<%= link_to "Admin_HomePage", :controller => "admins", :action => "index" %>

<style type="text/css">
td
{
    padding:0 1px 0 0px;

}
th
{
  text-align: left;
  padding:0 10px 5px 0px;
}
</style>

<h2>List of all librarians</h2>
<table>
  <th>Name:</th>
  <th>Email:</th>
  <th>Library Id:</th>
<% @librarian.each do |librarian| %>
  <tr>
    <td><%=librarian.name %></td>
    <td><%=librarian.email %></td>
    <td> <%=librarian.library_id[:university]%></td>
    <td><%= link_to "Update", edit_student_path([:student_id]) %><td>
  </tr>
<%end %>
</table>

